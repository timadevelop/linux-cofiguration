! red
URxvt.color1  : #705050
URxvt.color9  : #DCA3A3
! green
URxvt.color2  : #60B48A
URxvt.color10 : #72D5A3
! yellow
URxvt.color3  : #DFAF8F
URxvt.color11 : #F0DFAF
! blue
URxvt.color4  : #9AB8D7
URxvt.color12 : #94BFF3
! magenta
URxvt.color5  : #DC8CC3
URxvt.color13 : #EC93D3
! cyan
URxvt.color6  : #8CD0D3
URxvt.color14 : #93E0E3
