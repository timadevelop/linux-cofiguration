Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

URxvt*font: xft:Hack: size=8:antialias=true, \
            xft:fontawesome:size:12, \
            xft:Hack Nerd Font Mono:size:12
URxvt*boldFont: xft:Hack: size=8:antialias=true:style=bold, \
                xft:fontawesome:size:12, \
                xft:Hack Nerd Font Mono:size:12
#URxvt*boldFont: xft:Source Code Pro:bold:pixelsize=10
#URxvt*letterSpace: -1 # break some symbols

################
# URXVT settings
################
URxvt.borderless: true
URxvt.cursorColor:  #afbfbf

#URxvt*.transparent: true
#URxvt*.shading: 20
#Urxvt*depth: 32
#Urxvt*background: rgba:0000/0000/0200/c800
#URxvt.depth:                32
URxvt.geometry:    90x30
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0


URxvt*saveLines: 12000

# scrollbar - true (default) or false
URxvt*scrollBar: false
##
## Dark pastels
##
##
URxvt*background: #2C2C2C
URxvt*foreground: #DCDCCC
! black
URxvt.color0  : #3F3F3F
URxvt.color8  : #709080
! red
URxvt.color1  : #705050
URxvt.color9  : #DCA3A3
! green
URxvt.color2  : #60B48A
URxvt.color10 : #72D5A3
! yellow
URxvt.color3  : #DFAF8F
URxvt.color11 : #F0DFAF
! blue
URxvt.color4  : #9AB8D7
URxvt.color12 : #94BFF3
! magenta
URxvt.color5  : #DC8CC3
URxvt.color13 : #EC93D3
! cyan
URxvt.color6  : #8CD0D3
URxvt.color14 : #93E0E3
! white
URxvt.color7  : #DCDCCC
URxvt.color15 : #FFFFFF


!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
! URxvt.url-select.underline: true
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
! URxvt.resize-font.smaller:	C-Down
! URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.matcher.button:	1

rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency:	false
! rofi.lines:		3
! rofi.bw:		0
! rofi.opacity:		"10"
! rofi.hide-scrollbar:	true
! rofi.width:		30


