!#
!# Dark pastels
!#
!#
*background: #2C2C2C
*foreground: #DCDCCC

! black
*.color0  : #4b4b4b
! 3F3F3F
*.color8  : #709080

! red
*.color1  : #DCA3A3
*.color9  : #DCA3A3

! green
*.color2  : #60B48a
*.color10 : #72D5A3

! yellow
*.color3  : #DFAF8F
*.color11 : #F0DFAF

! blue
*.color4  : #9AB8D7
*.color12 : #94BFF3

! magenta
*.color5  : #DC8CC3
*.color13 : #EC93D3

! cyan
*.color6  : #8CD0D3
*.color14 : #93E0E3

! white
*.color7  : #DCDCCC
*.color15 : #FFFFFF


*.cursorColor:  #94BFF3

! fonts
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

!
! URXVT settings
!
URxvt*font: xft:Hack\ Nerd\ Font:size=9:antialias=true, \
            xft:Hack\ Nerd\ Font\ Mono

URxvt*boldFont: xft:Hack:size=9:antialias=true:style=bold, \
                xft:Hack\ Nerd\ Font\ Mono:size=12:style=bold

URxvt.borderless: true

! cursor underline  
URxvt*cursorUnderline:      true
! cursor blinking 
URxvt*cursorBlink:          false

!URxvt*.transparent: true
!URxvt*.shading: 20
!Urxvt*depth: 32
!Urxvt*background: rgba:0000/0000/0200/c800
!URxvt.depth:                32
URxvt.geometry:    90x30
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt*saveLines: 12000
! scrollbar - true (default) or false
URxvt*scrollBar: false


!! Extensions
URxvt.perl-ext-common:default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL: #4682B4
!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
! URxvt.url-select.underline: true
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.matcher.button:	1


!
! rofi settings
!
rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency:	false
! rofi.lines:		3
! rofi.bw:		0
! rofi.opacity:		"10"
! rofi.hide-scrollbar:	true
! rofi.width:		30

