! fonts
Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

!
! URXVT settings
!
*font: xft:Hack\ Nerd\ Font:size=9:antialias=true, \
            xft:Hack\ Nerd\ Font\ Mono, \
            xft:Siji:pixelsize=10

URxvt*boldFont: xft:Hack:size=9:antialias=true:style=bold, \
                xft:Hack\ Nerd\ Font\ Mono:size=12:style=bold, \
                xft:Siji:pixelsize=10

URxvt.borderless: true

! cursor underline  
URxvt*cursorUnderline:      true
! cursor blinking 
URxvt*cursorBlink:          false

!URxvt*.transparent: true
!URxvt*.shading: 20
!Urxvt*depth: 32
!Urxvt*background: rgba:0000/0000/0200/c800
!URxvt.depth:                32
URxvt.geometry:    90x30
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0
URxvt*saveLines: 12000
! scrollbar - true (default) or false
URxvt*scrollBar: false


!! Extensions
URxvt.perl-ext-common:default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL: #4682B4
!! url-select
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
! URxvt.url-select.underline: true
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.matcher.button:	1


!
! rofi settings
!
rofi.fake-transparency:	false
rofi.lines: 20
rofi.width: 50
rofi.opacity: 80
